import labs/rl/NemoV4/validate;
import labs/rl/NemoV4/vm;

main() {
    programm = 
    "
		VAR x : INT;
        {
            x := 3 + 1;  
        }
    "

    prog = parse(programm);

    println(prog);
    println("");

    correct = checkProgram(prog);
    if (correct) {
        code = translateToVMCode(prog);
        println(code);
        println("");

        final = executeProgram(code);
        println(final);
    }

    quit(0);
}