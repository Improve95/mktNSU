import labs/block2/rational/polynomRat;

main() {
    testPolynom = "(x + 1) / x";

    ratExpr = s2re(testPolynom);

    polynomExpr = re2polynom(ratExpr);

    simpleExpr = re2simpleRe(polynomExpr);
    println("polynom: " + re2s(simpleExpr, 0));

    vars = [Pair("x", RatNum(2, 1))];
    tree = setTreeValues(TreeEmpty(), vars);

    res = calcRe(simpleExpr, tree);
    println("res: " + reNum2s(res));

    quit(0);
}
