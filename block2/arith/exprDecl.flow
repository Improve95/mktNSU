import string;
import math/math;
import ds/tree;
import lingo/pegcode/driver;
import fs/filesystem;

export {
    /* lingo */
    infixGrammar = compilePegGrammar("#include labs/block2/arith/infix.lingo");

    /* Infix */
    InfixExpr ::= Const, Var, Frac, Unary, Log, BinOp, Null;
	Const(number : double);
    Var(name : string);
    Frac(left: InfixExpr, right: InfixExpr);
    Unary(binOp : InfixExpr);
    Log(e : InfixExpr);
    BinOp(name : string, left: InfixExpr, right: InfixExpr);
    Null();
}