import labs/block2/rational/rational;

inputVariablesIntoTree(vars : [string], values : [double]) -> Tree<string, double> {
    foldi(vars, makeTree(), \i, tree, variable -> {
        setTree(tree, variable, values[i])
    });
}

main() {
    // testRational = "- ((((5 * 3) / (4 * 2)) / 3) * a)";
    testRational = "(1/(x-(2/4)))";
    params = setTreeValues(makeTree(), [Pair("x", ratNum(5, 1))]);

    expRat = parse(testRational);
    println(expr2s(expRat));

    solveRat = calculateRat(expRat, params);
    println(ratNum2s(solveRat));

    quit(0);
}
